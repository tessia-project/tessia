    <dhcp_options>
      <dhclient_client_id/>
      <dhclient_hostname_option>AUTO</dhclient_hostname_option>
    </dhcp_options>
    <dns>
      <dhcp_hostname config:type="boolean">false</dhcp_hostname>
      {% for iface in config["ifaces"] %}
	  {% if iface["is_gateway"] %}
	  {% set host_domain = config["hostname"].split('.', 1) %}
	  <!-- need to fix this -->
	  <domain>{{ host_domain[1] }}</domain>
	  <hostname>{{ host_domain[0] }}</hostname>
      <nameservers config:type="list">
        <nameserver>{{ iface["dns_1"] }}</nameserver>
      </nameservers>
	  {% endif %}
	  {% endfor %}
      <resolv_conf_policy>auto</resolv_conf_policy>
      <write_hostname config:type="boolean">false</write_hostname>
    </dns>
    <interfaces config:type="list">
      <!-- not working for Hypersockets -->
	  {% for iface in config["ifaces"] %}
      {% set device_nr = 0 %}
      <interface>
        <bootproto>static</bootproto>
		<device>eth{{ device_nr }}</device>
	    <ipaddr>{{ iface["ip"] }}</ipaddr>
        <lladdr>{{ iface["mac_addr"] }}</lladdr>
        <netmask>{{ iface["mask"] }}</netmask>
        <prefixlen>{{ iface["mask_bits"] }}</prefixlen>
        <startmode>auto</startmode>
      </interface>
      {% set device_nr = device_nr + 1 %}
	  {% endfor %}
      <interface>
        <bootproto>static</bootproto>
        <broadcast>127.255.255.255</broadcast>
        <device>lo</device>
        <firewall>no</firewall>
        <ipaddr>127.0.0.1</ipaddr>
        <netmask>255.0.0.0</netmask>
        <network>127.0.0.0</network>
        <prefixlen>8</prefixlen>
        <startmode>nfsroot</startmode>
        <usercontrol>no</usercontrol>
      </interface>
    </interfaces>
    <ipv6 config:type="boolean">true</ipv6>
    <keep_install_network config:type="boolean">false</keep_install_network>
    <managed config:type="boolean">false</managed>
	{% if config["system_type"] == "LPAR" %}
    <net-udev config:type="list">
      {% for iface in config["ifaces"] %}
      {% set device_nr = 0 %}
      <rule>
		  <name>eth{{ device_nr }}</name>
        <rule>KERNELS</rule>
		<value>{{ iface["attributes"]["ccwgroup"].split(',')[0] }}</value>
      </rule>
      {% set device_nr = device_nr + 1 %}
	  {% endfor %}
    </net-udev>
	{% endif %}
    <routing>
      <ipv4_forward config:type="boolean">false</ipv4_forward>
      <ipv6_forward config:type="boolean">false</ipv6_forward>
      <routes config:type="list">
        <route>
          <destination>default</destination>
          <device>eth0</device>
          {% for iface in config["ifaces"] %}
          {% if iface["is_gateway"] %}
		  <gateway>{{ iface["gateway"] }}</gateway>
          {% endif %}
	      {% endfor %}
          <netmask>-</netmask>
        </route>
      </routes>
    </routing>
    <s390-devices config:type="list">
      {% for iface in config["ifaces"] %}
      <listentry>
		{% if config["system_type"] == "LPAR" %}
		<chanids>{{ iface["attributes"]["ccwgroup"]|replace(',', ' ') }}</chanids>
        <portname>OSAPORT</portname>
        <type>qeth</type>
		{% endif %}
      </listentry>
      {% endfor %}
    </s390-devices>
  </networking>
