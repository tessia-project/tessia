<?xml version="1.0"?>
<!DOCTYPE profile>
<profile xmlns="http://www.suse.com/1.0/yast2ns" xmlns:config="http://www.suse.com/1.0/configns">
  <add-on>
    <add_on_products config:type="list"/>
  </add-on>
  <!-- Bootloader configuration -->
    <bootloader>
    <device_map config:type="list">
      <device_map_entry>
	    <firmware>hd0</firmware>
	    {% for volume in config["svols"] %}
			{% if volume["is_root"] %}
			    {% if config["system_type"] == "KVM" %}
				<linux>/dev/vda</linux>
				{% elif volume["type"] == "DASD" %}
				<linux>/dev/dasda</linux>
				{% elif volume["specs"]["multipath"] %}
				<linux>{{ volume["system_attributes"]["device"] }}</linux>
				{% else %}
				<linux>/dev/sda</linux>
				{% endif %}
			{% endif %}
		{% endfor %}
      </device_map_entry>
    </device_map>
    <global>
      <activate>false</activate>
	  <append> hvc_iucv=8 TERM=dumb</append>
      <boot_boot>false</boot_boot>
      <boot_extended>false</boot_extended>
      <boot_mbr>false</boot_mbr>
      <boot_root>false</boot_root>
      <default>0</default>
      <distributor/>
      <failsafe_disabled>true</failsafe_disabled>
      <gfxmode>auto</gfxmode>
      <os_prober>false</os_prober>
      <terminal>console</terminal>
      <timeout config:type="integer">8</timeout>
      <vgamode/>
    </global>
    <loader_type>grub2</loader_type>
  </bootloader>
  {% for volume in config["svols"] %}
	<dasd>
      <devices config:type="list">
	  {% if config["system_type"] == "LPAR" and volume["type"] == "DASD" %}
	    <listentry>
		  <channel>0.0.{{ volume["volume_id"] }}</channel>
		  <diag config:type="boolean">false</diag>
		</listentry>
	  {% endif %}
	  </devices>
      <format_unformatted config:type="boolean">false</format_unformatted>
	</dasd>
  {% endfor %}
  <deploy_image>
    <image_installation config:type="boolean">false</image_installation>
  </deploy_image>
  <firewall>
    <FW_ALLOW_FW_BROADCAST_DMZ>no</FW_ALLOW_FW_BROADCAST_DMZ>
    <FW_ALLOW_FW_BROADCAST_EXT>no</FW_ALLOW_FW_BROADCAST_EXT>
    <FW_ALLOW_FW_BROADCAST_INT>no</FW_ALLOW_FW_BROADCAST_INT>
    <FW_ALLOW_FW_TRACEROUTE>yes</FW_ALLOW_FW_TRACEROUTE>
    <FW_ALLOW_PING_FW>yes</FW_ALLOW_PING_FW>
    <FW_CONFIGURATIONS_DMZ>sshd vnc-httpd vnc-server</FW_CONFIGURATIONS_DMZ>
