      {% set devno = volume["specs"]["adapters"][0]["devno"] %}
	  {% set wwpn = volume["specs"]["adapters"][0]["wwpns"][0] %}
      <listentry>
		<controller_id>{{ devno }}</controller_id>
		<fcp_lun>0x{{ volume["volume_id"] }}</fcp_lun>
		<wwpn>0x{{ wwpn }}</wwpn>
      </listentry>
	  {% endif %}
	{% endfor %}
    </devices>
  </zfcp>
