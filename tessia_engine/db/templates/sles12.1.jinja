    <FW_CONFIGURATIONS_INT>sshd vnc-httpd vnc-server</FW_CONFIGURATIONS_INT>
    <FW_IGNORE_FW_BROADCAST_DMZ>no</FW_IGNORE_FW_BROADCAST_DMZ>
